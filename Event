namespace App;

enum EventType
{
    RegistrationRequested,
    RegistrationAccepted,
    AppointmentRegistered,
    AppointmentModified,
    AppointmentApproved,
    PermissionGranted,
    LocationAdded,
    JournalViewed,
    Login,
    Logout,
}



